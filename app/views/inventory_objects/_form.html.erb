  <%= semantic_form_for @inventory_object do |f| %>
    <%= f.inputs do %>
    <% unless @inventory_object.new_record? %>
      <%= f.input :inventory_object_version, :as => :select, 
        :collection => @inventory_object.inventory_object_version.inventory_object_type.versions.all %>
    <% else %>
      <%= f.input :inventory_object_version, :as => :select, :collection => InventoryObjectVersion.type_sort.all %>
    <% end %>
		<%= f.input :id1 %>
		<%= f.input :id2 %>
		<%= f.input :id3 %>
    <%= f.input :status_tag_list, 
                :as => :autocomplete, 
                :url => autocomplete_tag_name_inventory_objects_path, 
                :input_html => {"data-delimiter" => ", "} %>
    <% end %>
    <%= f.actions %>
  <% end %>
